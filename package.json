{
    "name": "log-analyzer",
    "displayName": "Salesforce - Debug Log Analyzer",
    "description": "Goal of this extension is to be able to break down log files from Salesforce (still in developmental mode)",
    "version": "0.3.1",
    "engines": {
        "vscode": "^1.98.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "log-analyzer.analyzeDebugLog",
                "title": "Analyze Debug Log"
            }
        ]
    },
    "publisher": "jairocal",
    "scripts": {
        "install:all": "npm install && cd webview-ui && npm install",
        "build:webview": "cd webview-ui && npm run build",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-react": "^7.26.3",
        "@eslint/js": "^9.22.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.97.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "babel-loader": "^9.2.1",
        "css-loader": "^6.8.1",
        "eslint": "^9.22.0",
        "eslint-plugin-react": "^7.37.4",
        "globals": "^16.0.0",
        "prettier": "^3.5.2",
        "prettier-plugin-apex": "^2.2.4",
        "style-loader": "^3.3.1"
    },
    "dependencies": {
        "@vscode/vsce": "^3.3.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    }
}
