{
    "name": "log-analyzer",
    "displayName": "Log Analyzer",
    "description": "Goal of this extension is to be able to break down log files from Salesforce",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "log-analyzer.analyzeDebugLog",
                "title": "Analyze Debug Log"
            },
            {
                "command": "vscode-react-webview.openWebview",
                "title": "Open React Webview"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test",
        "build": "webpack --mode production",
        "watch": "webpack --watch"
    },
    "devDependencies": {
        "webpack": "^4.0.0",
        "webpack-cli": "^3.0.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.97.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.19.0",
        "prettier": "^3.5.2",
        "prettier-plugin-apex": "^2.2.4",
        "babel-loader": "^8.0.0",
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0"
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    }
}
